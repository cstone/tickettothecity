<h1>Listing events</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Details</th>
    <th>Event image</th>
    <th>Venue</th>
    <th>Price</th>
    <th>Address</th>
    <th>Phone</th>
    <th>Website</th>
    <th>Area</th>
    <th>Type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= event.title %></td>
    <td><%= event.details %></td>
    <td><%= event.event_image %></td>
    <td><%= event.venue %></td>
    <td><%= event.price %></td>
    <td><%= event.address %></td>
    <td><%= event.phone %></td>
    <td><%= event.website %></td>
    <td><%= event.area_id %></td>
    <td><%= event.type_id %></td>
    <td><%= link_to 'Show', event %></td>
    <td><%= link_to 'Edit', edit_event_path(event) %></td>
    <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Event', new_event_path %>
